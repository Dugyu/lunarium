{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "composite": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@lynx-js/react",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "bundler",

    "noEmit": true,
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
}
